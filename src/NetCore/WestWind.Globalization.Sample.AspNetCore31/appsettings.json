{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "DbResourceConfiguration": {
    "ResourceAccessMode": "DbResourceManager",
    "ConnectionString": "server=.;database=localizations;integrated security=true",
    "DataProvider": "SqlServer",
    "ResourceTableName": "Localizations_AppSettingsJson",
    "StronglyTypedGlobalResource": "~/Properties/Resources.cs",
    "ResourceBaseNamespace": "AppResources",
    "ResxExportProjectType": "Project",
    "ResxBaseFolder": "~/Properties/",
    "AddMissingResources": false,
    "ActiveConfiguration": null,
    "LocalizationFormWebPath": "~/LocalizationAdmin/",
    "GoogleApiKey": "Optional but recommended! Use UserSecrets: DbResourceConfiguration:GoogleApiKey",
    "BingClientId": "Use UserSecrets DbResourceConfiguration:BingClientId",
    "DeepLApiKey": "User UserSecrets DbResourceConfiguration:DeepLApiKey"
  },
  "LiveReload": {
    "LiveReloadEnabled": false,
    "ClientFileExtensions": ".cshtml,.css,.js,.htm,.html,.ts",
    "ServerRefreshTimeout": 3000
  }
}



